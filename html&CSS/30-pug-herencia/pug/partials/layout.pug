        meta(charset=charset)
        if(page == 1)
            title #{titulo}
        else 
            h1 #{titulo_pagina} | #{titulo}
        link(rel="stylesheet", href="./css/Stylepug.css")
    body
        include ../partials/nuevoheader.pug
        main
            block main
        include ../partials/footer.pug